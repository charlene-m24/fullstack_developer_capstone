[flake8]
ignore = E203, W503, E117, E128, E131, E201, E225, E231, E251, E265, E275, E302, E303, E501, E712, E722, F401, F811, F841, W291, W292, W293

exclude =
    venv,
    .venv,
    djangoenv,
    __pycache__,
    migrations,
    manage.py,
    node_modules,
    server/frontend/node_modules,
    */node_modules/*
